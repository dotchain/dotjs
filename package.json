{
  "name": "dotjs",
  "version": "0.0.1",
  "description": "Distributed sync using operational transformations",
  "main": "esmloader.js",
  "module": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "jsdoc core/*.js",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "eslint --env mocha --env es6 core/ test/",
    "pretty": "find . -name '*.js' | grep -v node_modules | xargs yarn prettier --write",
    "test": "nyc mocha --require esm test/**/*_test.js && karma start --e2e",
    "mocha": "mocha --require esm test/**/*_test.js",
    "karma": "karma start",
    "e2e": "karma start --e2e"
  },
  "nyc": {
    "include": [
      "core/*.js",
      "streams/*.js",
      "session/*.js"
    ],
    "sourceMap": false,
    "instrument": true
  },
  "eslintIgnore": [
    "test/session/golden.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotchain/dotjs.git"
  },
  "keywords": [
    "dot",
    "operational-transformation",
    "ot",
    "js",
    "es6"
  ],
  "author": "VK",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dotchain/dotjs/issues"
  },
  "homepage": "https://github.com/dotchain/dotjs#readme",
  "devDependencies": {
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "codecov": "^3.3.0",
    "cors": "^2.8.5",
    "eslint": "^5.16.0",
    "esm": "^3.2.22",
    "express": "^4.16.4",
    "karma": "^4.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "^6.1.4",
    "node-fetch": "^2.3.0",
    "nyc": "^14.0.0",
    "prettier": "1.17.0"
  }
}
